{
  "manifest_version": 2,
  "name": "Phantom Form",
  "description": "This extension will open a web form for submitting artifacts to Phantom for investigation",
  "version": "1.0",
  "browser_action": {
    "default_icon": "images/phantom16.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/phantom16.png",
    "48": "images/phantom48.png",
    "128": "images/phantom128.png"
  },
    "permissions": [
    "activeTab", "*://192.168.1.112/*", "*://*.virtual.optivmss.com/*"
  ],
  "background": {
    "scripts": ["thirdParty/jquery-3.1.0.min.js"]
  }
}